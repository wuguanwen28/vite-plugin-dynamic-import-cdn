  "homepage": "https://github.com/wuguanwen28/vite-plugin-dynamic-import-cdn#readme",
  "dependencies": {
    "rollup-plugin-external-globals": "^0.12.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/node": "^22.9.0",
    "prettier": "^3.3.3",
